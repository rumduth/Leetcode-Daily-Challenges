# The rand7() API is already defined for you.
# def rand7():
# @return a random integer in the range 1 to 7

class Solution:
    def rand10(self):
        """
        :rtype: int
        """
        random = (rand7() - 1) * 7 + rand7()
        while random > 40:
            random = (rand7() - 1) * 7 + rand7()
        return math.ceil(random / 4) 


# The rand7() API is already defined for you.
# def rand7():
# @return a random integer in the range 1 to 7

class Solution:
    def rand10(self):
        """
        :rtype: int
        """
        random = (rand7() - 1) * 7 + rand7()
        while random > 40:
            random = (rand7() - 1) * 7 + rand7()
        return (random - 1) % 10 + 1
        
